<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Change <li> Color to Cyan</title>
</head>
<body>
    <ul>
        <li>Item 1</li>
        <li>Item 2</li>
        <li>Item 3</li>
    </ul>

    <script>
        // JavaScript code here
        const listItems = document.querySelectorAll("li");

        listItems.forEach((li) => {
          li.style.color = "cyan";
        });
    </script>
</body>
</html>
